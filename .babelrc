{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "env": {
    "development": {
      "plugins": [
        ["transform-define", {"process.env.NODE_ENV": "development"}]
      ]
    },
    "production": {
      "plugins": [
        ["transform-define", {"process.env.NODE_ENV": "production"}]
      ]
    }
  },
  "plugins": ["transform-runtime"]
}
